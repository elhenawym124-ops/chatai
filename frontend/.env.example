# API Configuration
REACT_APP_API_URL=http://localhost:3001/api
REACT_APP_WS_URL=ws://localhost:3001

# Facebook SDK
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id

# Google Services
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Payment Gateways
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
REACT_APP_PAYPAL_CLIENT_ID=your-paypal-client-id

# App Configuration
REACT_APP_APP_NAME="Communication Platform"
REACT_APP_APP_VERSION=1.0.0
REACT_APP_COMPANY_NAME="Your Company"

# Feature Flags
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_ECOMMERCE=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# File Upload
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Theme Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_PRIMARY_COLOR=#3B82F6
REACT_APP_SECONDARY_COLOR=#10B981

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token

# Error Tracking
REACT_APP_SENTRY_DSN=your-sentry-dsn

# Development
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=info

# PWA Configuration
REACT_APP_ENABLE_PWA=true
REACT_APP_PWA_NAME="Communication Platform"
REACT_APP_PWA_SHORT_NAME="CommPlatform"

# Localization
REACT_APP_DEFAULT_LANGUAGE=ar
REACT_APP_SUPPORTED_LANGUAGES=ar,en

# Real-time Features
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_HEARTBEAT_INTERVAL=30000

# Cache Configuration
REACT_APP_CACHE_DURATION=300000
